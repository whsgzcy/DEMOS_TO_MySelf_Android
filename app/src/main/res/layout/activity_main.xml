<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/json"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="拼接json" />

            <Button
                android:id="@+id/notification"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="notification" />

            <Button
                android:id="@+id/socket"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="socket" />

            <Button
                android:id="@+id/files"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="select files" />

            <Button
                android:id="@+id/anim"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="anim" />

            <Button
                android:id="@+id/dialog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="dialog" />

            <Button
                android:id="@+id/remarker"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="remarker" />

            <Button
                android:id="@+id/serial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="serialport" />

            <Button
                android:id="@+id/refresh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="recylerview 下拉刷新" />

            <Button
                android:id="@+id/events"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="事件总线" />

            <Button
                android:id="@+id/customview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="自定义控件" />

            <Button
                android:id="@+id/websocket"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="websocket" />

            <Button
                android:id="@+id/okhttp2websocket"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="okhttp2websocket" />

            <Button
                android:id="@+id/daemon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="daemon" />

            <Button
                android:id="@+id/xy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="xy" />

            <Button
                android:id="@+id/customShare"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="custom SharePrefence" />

            <Button
                android:id="@+id/lock"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="锁屏" />

            <Button
                android:id="@+id/base"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="基站数据" />

            <Button
                android:id="@+id/textAnim"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="字符动画" />

            <Button
                android:id="@+id/webview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="webview" />

            <Button
                android:id="@+id/simplewebview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="simplewebview" />

            <Button
                android:id="@+id/start_ofo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/start_ofo" />

            <Button
                android:id="@+id/btn_comment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="评分等级" />

            <Button
                android:id="@+id/speech"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="讯飞语音" />

            <Button
                android:id="@+id/baidu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="百度语音" />

            <Button
                android:id="@+id/count"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="到计时" />

            <Button
                android:id="@+id/btn_pwd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="密码明文切换状态" />

            <Button
                android:id="@+id/btn_camera"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="安卓拍照权限" />
            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/btn_indiactor"
                android:text="自定义数字指示器"
                />

            <Button
                android:id="@+id/yunzhisheng"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="免费离线语音合成" />

            <Button
                android:id="@+id/offinespeech"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="离线语音" />

            <Button
                android:id="@+id/data2server"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="数据逻辑到业务逻辑" />

            <Button
                android:id="@+id/open_anim"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="开机动画" />

            <Button
                android:id="@+id/test"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="测试" />

            <Button
                android:id="@+id/aidl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="aidl" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>
